  color: #d6535c
}
.mode_lanmu_act{
  color: #d6535c
}
.iconchahao {
animation: scaleDrew 1s ease-in-out infinite;
}
@keyframes scaleDrew {
/* 定义关键帧、scaleDrew是需要绑定到选择器的关键帧名称 */
    0% {
      transform: scale(1);
    }
    25% {
      transform: scale(1.05);
    }
    50% {
      transform: scale(1);
    }
    75% {
      transform: scale(1.05);
    }
  }
  .icon-bianji{
    font-size: 26rpx
  }
