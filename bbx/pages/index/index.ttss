  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.item_right_title_none{
  font-size: 34rpx;
  width: 99%;
  margin-bottom: 15rpx;
