<view class="user">
	<view class="getuser">
		<image class="user_img" src="{{userinfo.avatarUrl}}"></image>
		<view class="user_xq">
			<view style="font-size:38rpx">{{userinfo.nickName}}</view>
			<view style="font-size:28rpx">手机号：请绑定手机号</view>
		</view>
	</view>
	<view class="wx_compile">
		<view class="wx_compile_item">微信：请绑定微信
			<view class="user_bj">编辑</view>
		</view>
		<view class="wx_compile_item">未开通会员<view class="user_bj">开通</view>
		</view>
	</view>
</view>

<view class="user_remaining">
	<view class="remaining_yu">
		<view class="remainng_yue">
			<view>可提现金额</view>
			<view style="font-size:55rpx;font-weight:600">0.00</view>
		</view>
		<view class="mywallet">我的钱包</view>
	</view>
	<view class="wallet">
		<view class="wallet_item">
			<view style="font-size:38rpx;margin-top:10rpx">0.00</view>
			<view style="color:#999">总收益(元)</view>
		</view>
		<view class="wallet_item">
			<view style="font-size:38rpx;margin-top:10rpx">0.00</view>
			<view style="color:#999">今日预估收益(元)</view>
		</view>
		<view class="wallet_item">
			<view style="font-size:38rpx;margin-top:10rpx">0.00</view>
			<view style="color:#999">昨日收益(元)</view>
		</view>
	</view>
</view>
<!-- 官方团队指导 -->
<view class="guidance">
	<view class="guidance_title">官方团队指导</view>
	<view class="guidance_data">
		<view class="data_top">
			<view>团队负责人:</view>
			<view class="phone">联系他</view>
		</view>
		<view class="guidance_quan">
			<view class="quan_item">
				<text style="color:#f8dfa8;font-size:55rpx" class="iconfont icon-renmai"></text>
				人脉圈</view>
			<view class="quan_item"><text style="color:#f8dfa8;font-size:55rpx" class="iconfont icon-kefu"></text>官方客服</view>
			<view class="quan_item"><text style="color:#f8dfa8;font-size:45rpx;margin-right:10rpx" class="iconfont icon-xinxi"></text>
				<view></view>交流群</view>
		</view>
		<view class="btn">
			<view class="btn_item btn_yq">邀请好友</view>
			<view class="btn_item btn_vip">开通会员</view>
		</view>
	</view>
</view>
<!-- 其他功能 -->
<view class="workers">
	<view class="workes_title">其他功能</view>
	<view class="workes_main">
		<view class="workes_main_item">
			<view style="color:#fece3a" class="iconfont renmai icon-douyin"></view>
			<view class="workes_item_name">我的抖音号</view>
		</view>
		<view class="workes_main_item">
			<view style="color:#fc5a84" class="iconfont renmai icon-baobiao"></view>
			<view class="workes_item_name">收益报表</view>
		</view>
		<view class="workes_main_item">
			<view style="color:#adadad" class="iconfont renmai icon-hezuo"></view>
			<view class="workes_item_name">商务合作</view>
		</view>
		<view class="workes_main_item">
			<view style="color:#adadad" class="iconfont renmai icon-B-tuanduiguanli"></view>
			<view class="workes_item_name">团队管理</view>
		</view>
		<view class="workes_main_item">
			<view style="color:#adadad" class="iconfont renmai icon-tongzhi"></view>
			<view class="workes_item_name">系统通知</view>
		</view>
		<view class="workes_main_item">
			<view style="color:#adadad" class="iconfont renmai icon-jianyi"></view>
			<view class="workes_item_name">投诉建议</view>
		</view>
		<view class="workes_main_item">
				<view style="color:#adadad" class="iconfont renmai icon-guanyuwomen-copy"></view>
			<view class="workes_item_name">关于我们</view>
		</view>
		<view class="workes_main_item">
			<view style="color:#adadad" class="iconfont renmai icon-xitongshezhi"></view>
			<view class="workes_item_name">系统设置</view>
		</view>
	</view>
</view>

<view class="fotter">
	<view style="color:#b5b8c7">快狗推</view>
	<view style="color:#727888;font-size:24rpx;margin-top:10rpx">--让短视频更简单--</view>
</view>

<view wx:if="{{model}}" class="model" catchtouchmove="true">
	<button open-type="getUserInfo" bindgetuserinfo="bindGetUserInfo">用户授权</button>
</view>