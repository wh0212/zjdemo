<!-- 产品列表 -->
<view class="cp_list">
	<view class="cp_list_title">
		<view class="title_left">
			产品列表
			<text style="font-size:25rpx;color:#999">共<text style="color:#fdec4e">{{list.length}}</text>款小程序</text>
		</view>

		<navigator url="/pages/all_item/index">
			<view style="color:#fdec4e;font-size:30rpx">浏览全部单品<text class="iconfont icon-icon"></text></view>
		</navigator>
	</view>
	<view class="list">
		<!-- 产品 -->
		<view class="list_item" wx:for="{{list}}" wx:key="index">
			<view class="list_item_top">
				<view class="list_item_top_left">
					<image style="width:120rpx;height:120rpx" src="{{item.img}}"></image>
					<view class="item_top_left_xq">
						<view>{{item.title}}</view>
						<text class="item_top_left_xq_gj">{{item.type}}</text>
					</view>
				</view>
				<view class="list_item_top_right" data-item="{{item}}" bindtap="fundeatil">查看详情</view>
			</view>
			<view class="list_item_top_xq">{{item.describe}}</view>
			<view class="list_item_top_sy">
				<view class="item_top_sy_item">
					<view>昨日最高收益</view>
					<view>{{item.maxearnings}}元</view>
				</view>
				<view class="item_top_sy_item">
					<view>自推比例</view>
					<view>{{item.proportion}}%</view>
				</view>
				<view bindtap="promotion" style="color:#e83669" class="item_top_sy_item">
					<text class="iconfont icon-xiazai"></text>
					下载推广码
				</view>
			</view>
			<view wx:if="{{item.children.length>0}}">
				<view class="item_cp">精选<text style="color:#fdec4e">{{item.children.length}}</text>款子产品</view>
				<!-- 子产品 -->
				<view class="item_cp_zi" wx:for="{{item.children}}" wx:for-item="item2" wx:for-index="index2" wx:key="index">
					<image style="width:110rpx;height:110rpx" src="{{item2.img}}"></image>
					<view class="item_cp_zi_data">
						<view class="cp_zi_data">{{item2.title}}</view>
						<view class="cp_zi_bt">{{item2.share}}</view>
					</view>
					<view bindtap="promotion" style="color:#e83669">推广</view>
				</view>
				<view style="color:#fdec4e;line-height:60rpx;font-size:30rpx">推广更多单品</view>
			</view>
		</view>
	</view>
</view>
<!-- 下载推广码遮罩层 -->
<view catchtouchmove="preventD" bindtap="nodemdel" wx:if="{{model}}" class="tuiguang">
	<view catchtap="promotion" class="tuiguang_main">
		<view catchtap="aa" class="main_title">下载推广码</view>
		<view style="color:#888;font-size:30rpx" class="main_js">请下载对应APP的小程序推广码</view>
		<view style="color:red;font-size:30rpx" class="main_js">可绑定多个推广号，无上限</view>
		<view class="main_dy">
			<image class="dylogo" src="../../image/dy.png"></image>
			<text>抖音</text>
			<view catchtap="typept" data-pt="dy" class="main_dy_tg">下载推广码</view>
		</view>
		<view class="main_dy">
			<image class="dylogo" src="../../image/tt.png"></image>
			<text>头条</text>
			<view catchtap="typept" data-pt="tt" class="main_dy_tg">下载推广码</view>
		</view>
	</view>
</view>
<!-- 保存平台二维码 -->
<view catchtap="ewmnone" catchtouchmove="preventD" wx:if="{{ewmact}}" class="ewm">
	<view catchtap="{{true}}" class="ewm_main">
		<image style="width:90%;" data-img="{{'../../image/1.jpg'}}" bindlongtap="showBigImg" src="../../image/1.jpg"></image>
		<view class="saveImg">长按图片保存</view>
	</view>
</view>
