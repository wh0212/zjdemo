/* pages/cp_detail/index.wxss */
page {
  background: #14162b;
}

.top {
  width: 95%;
  margin: 20rpx auto;
  padding: 0rpx 20rpx;
}

.top_hander {
  display: flex;
  align-items: center;
  position: relative;
}

.top_hander_img {
  width: 120rpx;
  height: 120rpx;
}

.top_hander_bt {
  display: flex;
  flex-direction: column;
  color: #fff;
  padding-left: 20rpx;
  line-height: 60rpx;
}

.xiaz {
  position: absolute;
  right: 0;
  text-align: center;
  background: #ff2854;
  padding: 10rpx 25rpx;
  color: #fff;
  font-size: 24rpx;
  display: flex;
  align-content: center;
}

.top_header_txt {
  color: #595e7b;
  font-size: 28rpx;
  line-height: 45rpx;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

.list_children {
  width: 94%;
  margin: 20rpx;
}

.list_title {
  color: #fff;
  font-size: 34rpx;
}

.childrenList {
  width: 95%;
  background: #1f2447;
  padding: 20rpx;
  margin-top: 30rpx;
}

.item_cp_zi {
  width: 100%;
  height: 130rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.item_cp_zi_data {
  color: #Fff;
  width: 60%;
  line-height: 50rpx;
  margin-left: -60rpx;
  font-size: 30rpx;
}

.cp_zi_bt {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #8282a8;
  font-size: 26rpx;
}

.pushlist {
  width: 100%;
  text-align: center;
  color: #f7e861;
  font-size: 26rpx;
  line-height: 80rpx;
}

.earnings {
  width: 95%;
  margin: 20rpx auto;
}

.earnings_title {
  color: #fff;
}

.earnings_calendar {
  background: #212345;
  height: 50rpx;
  margin: 20rpx 0rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 50rpx;
  padding: 0rpx 20rpx;
  display: inline-block;
}

.earnings_xinxi {
  font-size: 26rpx;
  color: #8888b5;
}

.earnings_mx {
  width: 100%;
  padding: 20rpx 0rpx;
  margin: 20rpx auto;
  background: #232347;
}

.earnings_mx_head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #Fff;
  padding: 0rpx 20rpx;
  font-size: 30rpx;
}

.earnings_mx_sy {
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx;
  color: #767aa1;
  justify-content: space-between;
  line-height: 100rpx;
  font-size: 28rpx;
}

.earnings_mx_fwl {
  display: flex;
  align-items: center;
}

.earnings_mx_fwl_item {
  flex: 1;
  text-align: center;
}

.fwl_item_count {
  color: #fff;
  font-size: 37rpx;
}

.fwl_item_name {
  color: #767aa1 !important;
  font-size: 26rpx;
  margin-top: 10rpx;
}

.eanings_zoushi {
  width: 100%;
  padding: 20rpx 0rpx;
  margin: 30rpx auto;
  background: #232347;
}

.eanings_title {
  color: #Fff;
  font-size: 30rpx;
  padding: 0rpx 20rpx;
}

.eanings_zoushi_sy {
  padding: 0rpx 20rpx;
  color: #767aa1;
  line-height: 80rpx;
}

/* 图表 */
ec-canvas {
  width: 100%;
  height: 100%;
}

.container {
  height: 400rpx;
}
/* 下载推广码 */
.tuiguang {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.8);
}

.tuiguang_main {
  width: 80%;
  padding: 20rpx 0rpx;
  background: #fff;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  /* 定位父级的50% */
  top: 50%;
  transform: translate(-50%, -50%);
  /*自己的50% */
}
.main_title {
  text-align: center;
  font-size: 35rpx;
}
.main_js {
  text-align: center;
  line-height: 70rpx;
}
.main_dy{
  display: flex;
  align-items: center;
  width: 90%;
  margin: 20rpx auto;
  position: relative;
}
.dylogo{
  width: 100rpx;
  height: 100rpx;
  margin-right: 30rpx;
}
.main_dy_tg{
  float: right;
  right: 0;
  position: absolute;
  z-index: 9999999;
  font-size: 28rpx;
  background: #fe2c53;
  color: #fff;
  text-align: center;
  padding: 10rpx 20rpx;
}
.ewm {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.8);
  z-index: 99999;
}
.ewm_main {
  width: 80%;
  padding: 20rpx 0rpx;
  background: #fff;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  /* 定位父级的50% */
  top: 50%;
  transform: translate(-50%, -50%);
  /*自己的50% */
  text-align: center;
}
.saveImg{
  font-size: 30rpx;
  text-align: center;
}